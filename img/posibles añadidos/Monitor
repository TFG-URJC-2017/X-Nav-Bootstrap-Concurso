<script>
  'use strict';

  var app = {
    appUrl: 'web-app',
    apiUrl:  'http://api.icons8.com', // ссылка на api.icons8
    //siteUrl:  '', // ссылка на api.icons8

    auth: {
      rights: {"execute-api-iconmeta-add-tags":true,"execute-api-iconmeta-vote-tags":true,"execute-api-lightbox-create-bitmap-zip":true,"execute-api-lightbox-recolor-bitmap":true,"execute-api-iconmeta-clean-tags":false,"execute-api-lightbox-create-vector-zip":false,"execute-api-lightbox-create-web-font":false,"execute-api-lightbox-recolor-vector":false,"execute-api-task-append-tags":false,"execute-api-task-replace-tags":false,"download-all-bitmaps-images":false,"download-bitmaps-images":true,"download-bitmaps-zip":true,"download-vectors-images":false,"download-vectors-zip":false,"nolink-icon-usage":false},        // права пользователей
      license: {},      // лицензия пользователя
      isGuest: true,      // гость или авторизован
      loginUrl: '/login/?back=',  // url для логинации
      userId: '',

      // кодируем текущий url
      getRedirectUrl: function(action) {
        var hash = location.hash;
        if (action) {
          if (hash.indexOf('/action/') > -1) {
            hash = hash.substr(0, hash.indexOf('/action/') + 8) + action;
          } else {
            hash += '/action/' + action;
          }
        }
        return encodeURIComponent(
          location.protocol + '//' +
            location.hostname +
            location.pathname +
            location.search +
            (hash && hash.indexOf('#!') > -1 ? '_HASH_' + hash.substr(2) : '')
        );
      }
    },

    iconsCount: "8800+"
  };
  if (!app.auth.license.features) {
      app.auth.license.features = {
          nolink: false,
          updates: false,
          vector: false
      }
  }
  app.auth.license.expiry = app.auth.license.expiry*1000;
  app.auth.license.purchased = app.auth.license.purchased*1000;

  // GA tracking для обычных страниц
  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-362385-8']);
  _gaq.push(['_setDomainName', 'icons8.com']);
  _gaq.push(['_trackPageview']);
  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'stats.g.doubleclick.net/dc.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();
</script><script>
